table.table
  thead
    tr
      th(scope='col') F.O Name
      th(scope='col') NIN NUMBER
      th(scope='col') DATE OF BIRTH
      th(scope='col') GENDER
      th(scope='col') TELEPHONE NUMBER
      th(scope='col') WARD
      th(scope='col') RESIDENCE TYPE
      th(scope='col') PERIOD OF STAY IN THE WARD
      th(scope='col') DIRECTIONS TO HIS/HER HOME
      th(scope='col') ACTIVITIES INVOLVED IN
      th(scope='col') UNIQUE ID
      th(scope='col') DATE OF REGISTRATION
  tbody
      if farmerOnes.length
          each farmerOne in farmerOnes
              tr
                  td #{farmerOne.farmerOneName}
                  td #{farmerOne.foNin}
                  td #{farmerOne.foDob}
                  td #{farmerOne.gender}
                  td #{farmerOne.foTel}
                  td #{farmerOne.ward}
                  td #{farmerOne.residenceType}
                  td #{farmerOne.years}
                  td #{farmerOne.foDirections}
                  td #{farmerOne.foActivities}
                  td #{farmerOne.foUniqueID}
                  td #{farmerOne.foDor}          
      else
          p Sorry, there are no farmers in the database!
script(src='/CSS/Bootstrap/bootstrap-5.3.0-alpha1-dist/js/bootstrap.bundle.js')
script(src='/js/custom.js')

